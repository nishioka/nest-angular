{
  "name": "nest-angular",
  "version": "1.0.0",
  "description": "sample application",
  "main": "index.js",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "serve": "nx run-many --target=serve --projects=api,angular-sample --parallel=true",
    "serve:static": "cross-env STATIC_SERVE=true nx serve api",
    "serve:angular-sample": "nx serve angular-sample",
    "serve:api": "nx serve api",
    "e2e:api": "nx e2e api-e2e",
    "e2e:angular-sample": "nx e2e angular-sample-e2e",
    "build": "nx build",
    "test": "nx test",
    "populate": "cross-env DB_SYNC_FORCE=true nx serve populate",
    "prettier": "npx prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nishioka/nest-angular.git"
  },
  "keywords": [
    "nestjs",
    "angular"
