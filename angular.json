{
  "version": 1,
  "projects": {
    "angular-sample": {
      "projectType": "application",
      "root": "apps/angular-sample",
      "sourceRoot": "apps/angular-sample/src",
      "prefix": "sample",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/angular-sample",
            "index": "apps/angular-sample/src/index.html",
            "main": "apps/angular-sample/src/main.ts",
            "polyfills": "apps/angular-sample/src/polyfills.ts",
            "tsConfig": "apps/angular-sample/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/angular-sample/src/favicon.ico",
              "apps/angular-sample/src/assets"
            ],
            "styles": ["apps/angular-sample/src/styles.scss"],
            "scripts": [],
            "fileReplacements": [
              {
                "replace": "libs/shared/dto/src/lib/decorators-nest.ts",
                "with": "libs/shared/dto/src/lib/decorators-angular.ts"
              }
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/angular-sample/src/environments/environment.ts",
                  "with": "apps/angular-sample/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "angular-sample:build:production"
            },
            "development": {
              "browserTarget": "angular-sample:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "proxyConfig": "apps/angular-sample/proxy.conf.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angular-sample:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/angular-sample/src/**/*.ts",
              "apps/angular-sample/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/angular-sample"],
          "options": {
            "jestConfig": "apps/angular-sample/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "angular-sample-e2e": {
      "root": "apps/angular-sample-e2e",
      "sourceRoot": "apps/angular-sample-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/angular-sample-e2e/cypress.json",
            "devServerTarget": "angular-sample:serve:development",
            "tsConfig": "apps/angular-sample-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "angular-sample:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/angular-sample-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/api/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api"],
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-infra-server": {
      "root": "libs/api/infra-server",
      "sourceRoot": "libs/api/infra-server/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/api/infra-server/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/api/infra-server"],
          "options": {
            "jestConfig": "libs/api/infra-server/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-logging": {
      "root": "libs/api/logging",
      "sourceRoot": "libs/api/logging/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/api/logging/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/api/logging"],
          "options": {
            "jestConfig": "libs/api/logging/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-models": {
      "root": "libs/api/models",
      "sourceRoot": "libs/api/models/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/api/models/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/api/models"],
          "options": {
            "jestConfig": "libs/api/models/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-repositories": {
      "root": "libs/api/repositories",
      "sourceRoot": "libs/api/repositories/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/api/repositories/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/api/repositories"],
          "options": {
            "jestConfig": "libs/api/repositories/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "dto": {
      "root": "libs/shared/dto",
      "sourceRoot": "libs/shared/dto/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/shared/dto/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/dto"],
          "options": {
            "jestConfig": "libs/shared/dto/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "npm"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "angular-sample"
}
