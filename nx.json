{
  "npmScope": "sample",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "angular-sample": {
      "tags": []
    },
    "angular-sample-e2e": {
      "tags": [],
      "implicitDependencies": [
        "angular-sample",
        "dto"
      ]
    },
    "api": {
      "tags": [],
      "implicitDependencies": [
        "api-infra-server",
        "api-logging",
        "api-models",
        "api-repositories",
        "dto"
      ]
    },
    "api-infra-server": {
      "tags": []
    },
    "api-logging": {
      "tags": []
    },
    "api-models": {
      "tags": []
    },
    "api-repositories": {
      "tags": []
    },
    "dto": {
      "tags": []
    }
  }
}
